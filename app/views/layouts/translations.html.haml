!!!5
%html
  %head
    %title= 'Translations'
    = stylesheet_link_tag 'translations'
    = javascript_include_tag :defaults
    = csrf_meta_tag
    = render :partial => "/interpreter/shared/css"
  %body
    - flash.each do |name, msg|
      = content_tag :div, msg.html_safe, :id => "flash_#{name}"
    = yield
